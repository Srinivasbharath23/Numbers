#include<bits/stdc++.h>

using namespace std;

int main(){

int n,q;

cin>>n>>q;

long int arr[n + 1]={0};

for(int i = 1 ; i <= n ; i++){

long int temp;

cin>>temp;

arr[i] = arr [i-1] + temp;

}

while(q--){

int l,r;

cin>>l>>r;

cout<< (arr[r] - arr[l - 1]) / (r - l + 1)<<"\n";

}

return 0;

}
