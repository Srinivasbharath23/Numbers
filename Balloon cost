t = int(input())
for i in range(t):
    g,b = map(int,input().split(' '))
    n = int(input())
    cost = 0
    for j in range(n):
        inp1,inp2 = map(int,input().split(" "))
        if(g>b):
            res = (inp1*g + inp2*b)
        elif(b>g):
            res =(inp1*b + inp2*g)
        cost +=res
    print(cost)
