for i in range(int(input())):
    g,p = list(map(int , input().split()))
    users = int(input())
    x =0;y=0
    for i in range(users):
        i,j = list(map(int , input().split()))
        if(i==1 and j==0):
            x+=1
        elif(i==1 and j==1):
            x+=1
            y+=1
        elif(i==0 and j==1):
            y+=1
    print(min(g*x + p*y , g*y + p*x))
